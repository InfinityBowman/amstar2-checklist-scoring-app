{
  "name": "corates",
  "version": "0.0.1",
  "description": "An app for managing and scoring AMSTAR 2 checklists",
  "homepage": "https://InfinityBowman.github.io/amstar2-checklist-scoring-app",
  "scripts": {
    "dev": "vite dev",
    "docker": "docker compose up --build backend postgres electric -d",
    "build": "npm run build:packages && vite build",
    "build:packages": "for d in packages/*; do (cd \"$d\" && npm run build); done",
    "preview": "vite preview",
    "sw": "SOURCE_MAP=true SW_DEV=true vite build && vite preview",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write .",
    "test": "if [ \"$CI\" = \"true\" ]; then vitest run; else vitest --ui; fi"
  },
  "keywords": [],
  "author": "Jacob Maynard",
  "license": "MIT",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@electric-sql/client": "^1.0.14",
    "@electric-sql/solid": "0.0.1",
    "@solidjs/router": "^0.15.3",
    "@zag-js/password-input": "^1.22.1",
    "@zag-js/pin-input": "^1.24.2",
    "@zag-js/solid": "^1.24.2",
    "d3": "^7.9.0",
    "papaparse": "^5.5.3",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.9.9",
    "tinybase": "^6.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@solidjs/testing-library": "^0.8.10",
    "@tailwindcss/vite": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-solid": "^0.14.5",
    "fake-indexeddb": "^6.2.2",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.13",
    "vite": "^7.1.9",
    "vite-plugin-pwa": "^1.0.2",
    "vite-plugin-solid": "^2.11.8",
    "vitest": "^3.2.4"
  }
}
