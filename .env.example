# Copy this file to `.env` and fill in the required values.
# Database Configuration
POSTGRES_USER=corates
POSTGRES_PASSWORD=password
POSTGRES_DB=corates

# If you want real emails to be sent, use 'production' and fill in the email settings below
ENV=development

# Backend Configuration
DATABASE_URL=postgresql+asyncpg://corates:password@postgres:5432/corates
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,http://localhost:4173,http://127.0.0.1:3000,http://127.0.0.1:5173,http://127.0.0.1:4173,https://localhost,http://localhost
API_PREFIX=/api/v1
LOG_LEVEL=info
VITE_API_URL=http://localhost:8000/api/v1
SECRET_KEY=your_super_secret_key
ELECTRIC_URL=http://electric:3000
FRONTEND_URL=https://localhost/amstar2-checklist-scoring-app

# Email configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_super_secret_key
EMAIL_FROM=your_email@gmail.com
EMAIL_FROM_NAME=CoRATES